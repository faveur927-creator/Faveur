generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-openssl-1.1.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id             String  @id @default(cuid())
  name           String
  email          String  @unique
  hashedPassword String?
}
