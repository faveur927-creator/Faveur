generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-openssl-1.1.x"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id             String @id @default(cuid())
  email          String @unique
  name           String
  hashedPassword String
  createdAt      DateTime @default(now())
}
